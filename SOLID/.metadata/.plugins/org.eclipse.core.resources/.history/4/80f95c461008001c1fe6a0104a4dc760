
public class Main {

		// Liskov's Substitution Prenciple
	public static void main(String[] args) {

		Customer ibrahim = new IndividualCustomer(1, "İbrahim", "Gezer", "12121212121");

		Customer firma = new CorporateCustomer(1, "1010101010", "Ltd. Şti.");
		
		System.out.println(firma);
		System.out.println(ibrahim);

	}

}
